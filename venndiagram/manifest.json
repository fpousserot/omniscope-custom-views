        "mandatory": true,
        "validFieldTypes": [
          "NUMBER"
        ]
      },
      "filterBMax": {
        "displayName": "Filter B max",
        "detail": "Maximum value for the field filter, if not set, the set won't have a filter",
        "type": "TEXT"
