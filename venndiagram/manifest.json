        "validFieldTypes": [
          "NUMBER"
        ]
      },
      "filterAMax": {
        "displayName": "Filter A max",
        "detail": "Maximum value for the field filter, if not set, the set won't have a filter",
        "type": "TEXT"
      },
      "filterAMin": {
        "displayName": "Filter A min",
        "detail": "Minimum value for the field filter, if not set, the set won't have a filter",
        "type": "TEXT"
      },
      "filterBField": {
        "displayName": "Filter B field",
        "type": "FIELD",
