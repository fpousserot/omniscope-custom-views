{
  "name": "France Choropleth",
  "frameworkVersion": "v1",
  "icon": "icon.svg",
  "tags": ["map", "custom", "france"],
  "sandbox": false,
  "autoPane": true,
  "autoQuery": true,
  "dataLimit": 10000,
  "supportsSelection": true,
  "options": {
    "items": {
      "measures": {
        "displayName": "Measure",
        "type": "MEASURE",
        "mandatory": true
      },
      "paneX": {
        "displayName": "Pane X",
        "type": "GROUPING",
        "list": true
      },
      "paneY": {
        "displayName": "Pane Y",
        "type": "GROUPING",
        "list": true
      },
      "split": {
        "displayName": "Split",
        "type": "GROUPING"
      }
    },
    "structure": {
      "toolbar": [
        "measures",
        "split"
      ],
      "x": [
        "paneX"
      ],
      "y": [
        "paneY"
      ]
    }
  },
  "defaultOptions": {
    "items": {},
    "pane": {
      "yAxisHeaderWidth": 75,
      "paneWidth": 100,
      "paneHeight": 100,
      "gridLineThickness": 1,
      "headerStyle": {},
      "tileHeaderAlignment": "CENTER",
      "xHeaderPlacement": "TOP",
      "yHeaderPlacement": "LEFT",
      "showColourKeyInEveryPane": false,
      "enableTouchScrolling": false
    }
  },
  "pane": {
    "minWidth": 50,
    "minHeight": 50,
    "maxCells": 20
  }
}