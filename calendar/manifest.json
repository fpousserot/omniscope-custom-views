{
  "name": "Calendar",
  "frameworkVersion": "v1",
  "icon": "icon.svg",
  "thumbnail": "thumbnail.png",
  "tags": [
    "custom",
    "table",
    "time"
  ],
  "sandbox": false,
  "autoPane": true,
  "autoQuery": true,
  "dataLimit": 100000,
  "supportsSelection": true,
  "options": {
    "items": {
      "date": {
        "displayName": "Date",
        "type": "GROUPING",
        "mandatory": true,
        "validFieldTypes": [
          "DATE"
        ],
        "validGroupingTypes": [
          "UNIQUE_VALUES"
        ]
      },
      "value": {
        "displayName": "Value",
        "type": "MEASURE",
        "mandatory": true,
        "validMeasureResultTypes": [
          "NUMBER"
        ]
      }
    },
    "structure": {
      "toolbar": [
        "date",
        "value"
      ],
      "x": [],
      "y": []
    }
  },
  "defaultOptions": {
    "items": {},
    "pane": {
      "yAxisHeaderWidth": 75,
      "paneWidth": 100,
      "paneHeight": 100,
      "gridLineThickness": 1,
      "headerStyle": {},
      "tileHeaderAlignment": "CENTER",
      "xHeaderPlacement": "TOP",
      "yHeaderPlacement": "LEFT",
      "showColourKeyInEveryPane": false,
      "enableTouchScrolling": false
    }
  },
  "pane": {
    "minWidth": 50,
    "minHeight": 50,
    "maxCells": 20
  }
}